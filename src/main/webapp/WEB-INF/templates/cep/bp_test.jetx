#set(title="CEP埋点配置管理")
#set(navbar="servicelog")
#tag layout_block("headContent")
<style>
	#datatable td:nth-child(n+3) {
		text-align: right;
		width: 40px;
	}
</style>
#end
#tag layout_block("bodyContent")
<br/>
<div class="row">
	<div>
		<table id="datatable" class="table table-striped table-condensed">
			<thead>
			<tr>
				<td>
                    <button>预览埋点结果</button>
                    <button>添加埋点项</button>
                </td>
                <td colspan="2">
                    <div>
                        <div class="left">日志分隔类型</div>
                        <div class="right">
                        <select>
                            <option>Json格式</option>
                            <option>KV格式</option>
                        </select>
                        </div>
                    </div>
                </td>
			</tr>
            <tr>
                <th colspan="4">
                    <h3>期待埋点样例</h3>
                </th>
            </tr>
            <tr>
                <td colspan="4">
                    <textarea>请输入something...</textarea>
                </td>
            </tr>
			</thead>
			<tbody>
			</tbody>
		</table>
	</div>
</div>
<div class="row">
    <div>
        <table class="table table-striped table-condensed">
            <tr>
                <th colspan="4">
                    <h3>VS</h3>
                </th>
            </tr>
            <tr>
                <th colspan="4">
                    <h3>实际发布日志</h3>
                </th>
            </tr>
            <tr>
                <td>
                    <button>获取服务端日志</button>
                </td>
            </tr>
            <tr>
                <td>
                    <textarea></textarea>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="row">
    <div>
        <table class="table table-striped table-condensed">
            <tr>
                <td colspan="4">
                    <button>验证/对比埋点内容</button>
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="row">
    <div>
        结果对比表格
    </div>
</div>
<div>
    <div>
        <table class="table table-striped table-condensed">
            <tr>
                <td colspan="2">
                    <select>
                        <option>目标选择</option>
                    </select>
                    <button>发布埋点配置</button>
                </td>
                <td colspan="2"></td>
            </tr>
            <tr>
                <td colspan="2">
                    <button>生成一级仓库</button>
                    <button>预览报表结果</button>
                </td>
                <td colspan="2"></td>
            </tr>
        </table>
    </div>
</div>
#end
#tag layout_block("scriptContent")
<script>
</script>
#end
#include("/include/layout.jetx")
